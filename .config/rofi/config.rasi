configuration {
	modi: "combi";
  font: "Fira Code 14";
	combi-modi: "window,run";
  fixed-num-lines: true;
  max-history-size: 5;
  lines: 5;
	eh: 2;
	theme: "gruvbox-dark";
	kb-cancel: "Escape,Control+g,Control+bracketleft,Control+space,Super_L+d";
	kb-row-select: "";
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }
}
